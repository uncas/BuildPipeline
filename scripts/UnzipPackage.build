<?xml version="1.0"?>
<project name="BuildPipelinePackageHandling" default="preparePackage">

  <target name="init">
    <delete dir="work" />
    <mkdir dir="work" />
  </target>

  <target name="preparePackage" depends="init">
    <unzip zipfile="${zipfile.path}" todir="work" />
  </target>

</project>
